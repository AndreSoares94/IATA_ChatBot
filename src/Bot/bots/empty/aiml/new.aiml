<?xml version="1.0" encoding="UTF-8"?><aiml><category>	<pattern> OLA </pattern>	<template>		<condition name = "state" value = "neutral">Ola para si tambem</condition>		<condition name = "state" value = "stressado">Ola, vejo que esta stressado</condition>		<condition name = "state" value = "distraido">Ola, estava a ver que nao</condition>		<condition name = "state" value = "feliz">Ola, esta sorridente</condition>		<condition name = "state" value = "triste">Ola, espero que esteja tudo bem</condition>	</template></category><category>	<pattern> EMOTION * </pattern>	<template>		<think><set name = "state"><star/></set></think>	</template></category><category>	<pattern> Chamo-me * </pattern>	<template>		<condition name = "state" value = "neutral">Prazer em conhecer-te, <star/></condition>		<condition name = "state" value = "stressado">Acalme-se <star/></condition>		<condition name = "state" value = "distraido"><star/> demorou bastante a escrever</condition>		<condition name = "state" value = "feliz"><star/> vejo que estas feliz</condition>		<condition name = "state" value = "triste"><star/>vejo que estas triste</condition>	</template></category></aiml>